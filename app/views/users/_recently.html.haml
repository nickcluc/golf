%table#upcoming_table.tablesorter
  %thead
    %tr
      %th Date
      %th Course
      %th Tee Time
  %tbody
    - @user.not_scored_rounds.each do |not_scored|
      %tr
        %td= not_scored.round_date.strftime("%m/%d/%Y")
        %td= not_scored.course.name
        %td
          = link_to "#{not_scored.tee_time.strftime("%I:%M%p")}", round_path(not_scored)
