<table id="score_table" class="tablesorter">
  <thead>
    <tr>
      <th>Date</th>
      <th>Course</th>
      <th>Holes</th>
      <th>Score</th>
    </tr>
  </thead>
  <tbody>
    <% @user.player_rounds.includes(:round).each do |played| %>
      <tr>
        <td><%= played.round.round_date.strftime("%m/%d/%Y") %></td>
        <td><%= link_to "#{played.round.course.name}", course_path(played.round.course) %></td>
        <td><%= played.hole_count %></td>
        <td><%= played.score %></td>
      </tr>
    <% end %>
  </tbody>
</table>
