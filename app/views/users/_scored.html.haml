%table#score_table.tablesorter
  %thead
    %tr
      %th Date
      %th Course
      %th Holes
      %th Score
  %tbody
    - @user.player_rounds.includes(:round).each do |played|
      %tr
        %td= played.round.round_date.strftime("%m/%d/%Y")
        %td= link_to "#{played.round.course.name}", course_path(played.round.course)
        %td= played.hole_count
        %td= played.score
